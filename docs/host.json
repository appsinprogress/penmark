{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    },
    "logLevel": {
      "default": "Information"
    }
  },
  "customHandler": {
    "description": {
      "defaultExecutablePath": "node",
      "arguments": [
        "next_function.js"
      ]
    },
    "enableForwardingHttpRequest": true
  }
}